<!DOCTYPE html>
<html>
<body>
  <h1>节点管理</h1>
  <form action="/add" method="post">
    <select name="type">
      <option value="node">添加单个节点</option>
      <option value="subscription">添加订阅链接</option>
    </select>
    <input type="text" name="content" required>
    <button type="submit">提交</button>
  </form>
  
  <h2>现有节点 (<%= nodes.length %>)</h2>
  <ul>
    <% nodes.forEach(node => { %>
      <li><%= JSON.parse(node.config).ps %></li>
    <% }) %>
  </ul>
</body>
</html>
